# image names (CI / grader will set these)
BLUE_IMAGE=yourorg/blue-app:latest
GREEN_IMAGE=yourorg/green-app:latest

# which pool starts active (blue or green)
ACTIVE_POOL=blue

# release identifiers forwarded as X-Release-Id by the apps
RELEASE_ID_BLUE=blue-release-001
RELEASE_ID_GREEN=green-release-001

# optional app internal port
PORT=3000

# optional fine tune timeouts (strings with s for seconds)
PROXY_UPSTREAM_CONNECT_TIMEOUT=2s
PROXY_UPSTREAM_READ_TIMEOUT=5s
PROXY_NEXT_UPSTREAM_TIMEOUT=6s
